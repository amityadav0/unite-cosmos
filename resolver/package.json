{
  "name": "unite-cosmos-resolver",
  "version": "1.0.0",
  "description": "Cross-chain resolver for creating escrow contracts on Ethereum and Cosmos",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "demo": "node run-demo.js",
    "demo:eth-to-osmo": "node run-demo.js eth-to-osmo",
    "demo:osmo-to-eth": "node run-demo.js osmo-to-eth",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "ethers": "^6.8.1",
    "@cosmjs/cosmwasm-stargate": "^0.32.1",
    "@cosmjs/proto-signing": "^0.32.1",
    "@cosmjs/stargate": "^0.32.1",
    "@cosmjs/tendermint-rpc": "^0.32.1",
    "crypto": "^1.0.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "ethereum",
    "cosmos",
    "cross-chain",
    "escrow",
    "resolver",
    "atomic-swap"
  ],
  "author": "Unite Cosmos Team",
  "license": "MIT"
} 